#include<stdio.h>
#include<conio.h>
main()
{
int i=0,j=0,n;
printf("enter the number %d :",n);
scanf("%d",&n);
for(i=0;i<n;i++)
{
if(i+j)
{
printf("%d",n);
}
j=1
if(i+j)
{
printf("%d",n-j);
}
j=2
if(i+j)
{
printf("%d",n-j);
}
j=3
if(i+j)
{
printf("%d",n-j);
}
for(j=0;j<n;j++)
{
if(i+j==8)
{
printf("%d",n-4);
}
}
}
}

